class Vaga
  require '~/projects/arduino/banco.rb'
  require 'rubygems'
  require 'serialport'
  
  
  bd = Banco.new("postgres", "localhost", "projeto_final_development", "postgres", "starfaty")
  sp = SerialPort.new("/dev/ttyUSB0", 9600, 8, 1, SerialPort::NONE)
 
  while true do
	msg = sp.gets
unless msg.nil?
        printf("%s", msg.inspect)
puts 'oi'
end
	sleep(1)
   end
end
